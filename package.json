{
  "name": "irvin93d.github.io",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "fix": "FORCE_COLOR=1 run-s --print-label fix:*",
    "fix:eslint": "DEBUG=eslint:cli-engine eslint --fix .",
    "fix:prettier": "prettier --plugin-search-dir . --write .",
    "lint": "FORCE_COLOR=1 run-p --aggregate-output --print-label lint:*",
    "lint:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
    "lint:eslint": "DEBUG=eslint:cli-engine eslint . --max-warnings 0",
    "lint:prettier": "prettier --plugin-search-dir . --check ."
  },
  "devDependencies": {
    "@fontsource/inter": "^5.0.8",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@neoconfetti/svelte": "^1.0.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.25.1",
    "@types/cookie": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-svelte": "^2.33.2",
    "eslint-plugin-typescript-sort-keys": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-fa": "^3.0.4",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.6"
  },
  "type": "module",
  "engines": {
    "node": "v18.18.0",
    "pnpm": "v8.8.0"
  }
}
